{
  "name": "@fm-monorepo/prisma",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "prisma": "latest"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:reset": "prisma migrate reset --force && npm run db:generate",
    "db:deploy": "prisma migrate deploy"
  }
}
