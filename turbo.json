{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^db:generate", "^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^db:generate", "^build"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test:watch": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "type-check": {
      "dependsOn": ["db:generate", "^db:generate"]
    },
    "clean": {
      "cache": false
    }
  }
}
